<?xml version="1.0" encoding="utf-8"?>

<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="com.tetris" android:installLocation="auto">
<uses-sdk android:targetSdkVersion="9" android:minSdkVersion="9"/>
    <application android:label="Tetris on a Phone">
      <activity android:name="Tetris"
        android:screenOrientation="portrait"
        android:configChanges="keyboardHidden|orientation">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>
</manifest> 
